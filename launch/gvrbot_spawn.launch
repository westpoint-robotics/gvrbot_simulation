<!--URDF Launch -->
<launch>
 
    <!-- Convert an xacro and put on parameter server  -->
	<param name= "robot_description" command="$(find xacro)/xacro.py '$(find gvrbot_simulation)/models/robots/gvr_bot.urdf.xacro'"/>

    <!-- Spawn a robot into Gazebo -->
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-x 2 -param robot_description -urdf -model gvr02" />


</launch>
