<!--URDF Launch -->
<launch>
 
    <!-- Convert an xacro and put on parameter server  -->
	<param name= "robot_description02" command="$(find xacro)/xacro.py --inorder '$(find gvrbot_simulation)/models/robots/gvr_bot.urdf.xacro'"/>

    <!-- Spawn a robot into Gazebo -->
    <node name="spawn_urdf2" pkg="gazebo_ros" type="spawn_model" args="-z 0.3 -x 0 -param robot_description02 -urdf -model gvr02" />
    
    <include file="$(find gvrbot_simulation)/launch/xbox360_teleop.launch"/>
    

</launch>
